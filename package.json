{
  "name": "react-telephone-input",
  "version": "1.2.0",
  "description": "React component for entering and validating international telephone numbers. Inspired from the awesome jquery plugin for the same - https://github.com/Bluefieldscom/intl-tel-input.",
  "main": "lib/index.js",
  "scripts": {
    "build": "gulp clean && NODE_ENV=production gulp build",
    "examples": "gulp dev:server",
    "publish:site": "gulp publish:examples",
    "release": "gulp release",
    "start": "gulp dev",
    "test": "echo \"no tests yet\" && exit 0",
    "watch": "gulp watch:lib",
    "test": "mocha --compilers js:babel/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mukeshsoni/react-telephone-input"
  },
  "keywords": [
    "react-component",
    "react-telephone-input",
    "international-telephone-input",
    "react"
  ],
  "browserify": {
    "transform": [
      "reactify",
      "node-lessify"
    ]
  },
  "author": "Mukesh Soni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mukeshsoni/react-telephone-input/issues"
  },
  "homepage": "https://github.com/mukeshsoni/react-telephone-input",
  "devDependencies": {
    "gulp": "^3.9.0",
    "react-component-gulp-tasks": "^0.7.0",
    "jsdom": "^3.1.2",
    "mocha": "^2.2.5",
    "mocha-jsdom": "^0.4.0",
    "chai": "^3.0.0"
  },
  "dependencies": {
    "lodash": ">=2.4.1",
    "react": ">=0.12.1",
    "classnames": "^2.1.2",
    "react-onclickoutside": "^0.2.4"
  },
  "browserify-shim": {
    "classnames": "global:classNames",
    "react": "global:React",
    "lodash": "global:lodash"
  }
}
